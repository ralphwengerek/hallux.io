{
  "name": "hallux",
  "version": "1.0.0",
  "description": "A personal site",
  "main": "index.js",
  "scripts": {
    "serve": "cross-env NODE_ENV=development parcel src/index.html --open",
    "build": "cross-env NODE_ENV=production parcel build src/index.html --public-url ./",
    "pretest": "yarn run test:eslint",
    "test:eslint": "eslint src/ --ext .js --ext .jsx",
    "test": "jest --watch",
    "e2e": "cypress run --reporter junit --reporter-options mochaFile=results/output.xml",
    "lint:css": "stylelint './src/**/*.jsx'"
  },
  "dependencies": {
    "@material-ui/core": "^4.0.2",
    "@material-ui/icons": "^4.0.1",
    "@testing-library/react": "^8.0.1",
    "auth0-js": "^9.10.4",
    "auth0-lock": "^11.16.2",
    "axios": "^0.19.0",
    "dotenv": "^8.0.0",
    "immer": "^3.1.3",
    "jest-dom": "^3.5.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-async-generators": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-styled-components": "^1.10.0",
    "cross-env": "^5.2.0",
    "cypress": "^3.3.1",
    "docz": "^1.2.0",
    "docz-theme-default": "^1.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^2.4.0",
    "jest": "^24.8.0",
    "jest-styled-components": "^6.3.1",
    "lint-staged": "^8.2.0",
    "node-sass": "^4.12.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-eslint": "^1.0.7",
    "parcel-plugin-static-files-copy": "^2.1.2",
    "prettier": "^1.18.2",
    "react-hooks-testing-library": "^0.5.1",
    "react-hot-loader": "^4.11.0",
    "redux-devtools": "^3.5.0",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0"
  },
  "peerDependencies": {
    "react-test-renderer": "^16.8.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add ."
    ],
    "*.{scss,css,md}": [
      "prettier --write",
      "git add ."
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/src/**/index.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/react/cleanup-after-each",
      "jest-dom/extend-expect",
      "<rootDir>/src/config/setupTests.js"
    ]
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009"
      }
    }
  },
  "eslintIgnore": [
    "/src/config/setupTests.js",
    "/coverage/*",
    "/dist/*"
  ],
  "author": "Ralph Wengerek",
  "license": "ISC"
}
